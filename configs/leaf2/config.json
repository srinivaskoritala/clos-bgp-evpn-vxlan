{
  "nokia-srlinux:system": {
    "name": "leaf2",
    "network-instance": {
      "default": {
        "protocols": {
          "bgp": {
            "router-id": "1.1.1.11",
            "autonomous-system": 65001,
            "group": {
              "spine-group": {
                "export-policy": ["export-everything"],
                "import-policy": ["import-everything"],
                "peer-as": 65001
              }
            },
            "neighbor": {
              "1.1.1.6": {
                "group": "spine-group",
                "peer-address": "1.1.1.6"
              },
              "1.1.1.22": {
                "group": "spine-group",
                "peer-address": "1.1.1.22"
              }
            },
            "evpn": {
              "admin-state": "enable"
            }
          }
        }
      },
      "vlan-100": {
        "type": "mac-vrf",
        "protocols": {
          "bgp-evpn": {
            "bgp-instance": {
              "1": {
                "id": 1,
                "admin-state": "enable",
                "vxlan-interface": "vxlan1.100",
                "route-distinguisher": "1.1.1.11:100",
                "service-import-policy": ["import-vlan100"],
                "service-export-policy": ["export-vlan100"]
              }
            }
          }
        }
      }
    },
    "interface": {
      "ethernet-1/1": {
        "admin-state": "enable",
        "subinterface": {
          "1": {
            "admin-state": "enable",
            "ipv4": {
              "address": [
                {
                  "ip-prefix": "1.1.1.5/30"
                }
              ]
            }
          }
        }
      },
      "ethernet-1/2": {
        "admin-state": "enable",
        "subinterface": {
          "1": {
            "admin-state": "enable",
            "ipv4": {
              "address": [
                {
                  "ip-prefix": "1.1.1.21/30"
                }
              ]
            }
          }
        }
      },
      "ethernet-1/3": {
        "admin-state": "enable",
        "subinterface": {
          "1": {
            "admin-state": "enable",
            "vlan": {
              "encap": {
                "single-tagged": {
                  "vlan-id": 100
                }
              }
            },
            "bridge-table": {
              "mac-vrf": "vlan-100"
            }
          }
        }
      },
      "vxlan1": {
        "admin-state": "enable",
        "vxlan-interface": {
          "100": {
            "vxlan-interface-id": 100,
            "type": "bridged",
            "bridge-table": {
              "mac-vrf": "vlan-100"
            }
          }
        }
      }
    },
    "routing-policy": {
      "policy": {
        "export-everything": {
          "statement": [
            {
              "name": "accept-all",
              "action": {
                "accept": {}
              }
            }
          ]
        },
        "import-everything": {
          "statement": [
            {
              "name": "accept-all",
              "action": {
                "accept": {}
              }
            }
          ]
        },
        "export-vlan100": {
          "statement": [
            {
              "name": "export-vlan100",
              "action": {
                "accept": {}
              }
            }
          ]
        },
        "import-vlan100": {
          "statement": [
            {
              "name": "import-vlan100",
              "action": {
                "accept": {}
              }
            }
          ]
        }
      }
    }
  }
}
